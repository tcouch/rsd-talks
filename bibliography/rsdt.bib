@article{hawkes_asymptotic_1992,
	title = {Asymptotic {Distributions} of {Apparent} {Open} {Times} and {Shut} {Times} in a {Single} {Channel} {Record} {Allowing} for the {Omission} of {Brief} {Events}},
	volume = {337},
	issn = {0962-8436, 1471-2970},
	url = {http://rstb.royalsocietypublishing.org/content/337/1282/383},
	doi = {10.1098/rstb.1992.0116},
	abstract = {The openings and shuttings of individual ion channel molecules can be described by a Markov process with discrete states in continuous time. The predicted distributions of the durations of open times, shut times, bursts of openings, etc. are all described, in principle, by mixtures of exponential densities. In practice it is usually found that some of the open times, and the shut times, are too short to be detected reliably. If a fixed dead-time {\textless}latex{\textgreater}\${\textbackslash}tau \${\textless}/latex{\textgreater} is assumed then it is possible to define, as an approximation to what is actually observed, an `extended opening' or e-opening which starts with an opening of duration at least {\textless}latex{\textgreater}\${\textbackslash}tau \${\textless}/latex{\textgreater} followed by any number of openings and shuttings, all the shut times being shorter than {\textless}latex{\textgreater}\${\textbackslash}tau \${\textless}/latex{\textgreater}; the e-opening ends when a shut time longer than {\textless}latex{\textgreater}\${\textbackslash}tau \${\textless}/latex{\textgreater} occurs. A similar definition is used for e-shut times. The probability densities, f(t), of these extended times have previously been obtained as expressions which become progressively more complicated, and numerically unstable to compute, as t {\textless}latex{\textgreater}\${\textbackslash}rightarrow {\textbackslash}infty \${\textless}/latex{\textgreater}. In this paper we present, for the two-state model, an alternative representation as an infinite series of which a small number of terms gives a very accurate approximation of f(t) for large t. For the general model we present an asymptotic representation as a mixture of exponentials which is accurate for all except quite small values of t. Some simple model-independent corrections for missed events are discussed in relationship to the exact solutions.},
	language = {en},
	number = {1282},
	urldate = {2013-04-26},
	journal = {Phil. Trans. R. Soc. Lond. B},
	author = {Hawkes, A. G. and Jalali, A. and Colquhoun, D.},
	month = sep,
	year = {1992},
	pages = {383--404},
	file = {Hawkes et al. - 1992 - Asymptotic Distributions of Apparent Open Times an.pdf:/Users/jhn/Library/Application Support/Zotero/Profiles/8hs4j2nu.default/zotero/storage/TDH5WZI2/Hawkes et al. - 1992 - Asymptotic Distributions of Apparent Open Times an.pdf:application/pdf}
}

