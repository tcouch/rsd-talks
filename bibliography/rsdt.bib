	month = oct,
	year = {2014},
	pmid = {25079871},
	keywords = {angiogenesis, blood flow, Lattice-Boltzmann, mouse, retina, shear stress},
	pages = {20140543},
	file = {Bernabeu et al. - 2014 - Computer simulations reveal complex distribution o.pdf:/Users/jhn/Library/Application Support/Zotero/Profiles/8hs4j2nu.default/zotero/storage/BWGKP7PS/Bernabeu et al. - 2014 - Computer simulations reveal complex distribution o.pdf:application/pdf}
